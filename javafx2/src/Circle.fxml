<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Circle?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CircleController">
   <children>
      <Button fx:id="downBtn" layoutX="274.0" layoutY="324.0" mnemonicParsing="false" onAction="#moveDown" text="Qvemot" />
      <Button fx:id="upBtn" layoutX="274.0" layoutY="41.0" mnemonicParsing="false" onAction="#moveUp" text="Zemot" />
      <Circle fx:id="circle" fill="DODGERBLUE" layoutX="300.0" layoutY="200.0" radius="27.0" stroke="BLACK" strokeType="INSIDE" />
      <Button fx:id="leftBtn" layoutX="45.0" layoutY="187.0" mnemonicParsing="false" onAction="#moveLeft" text="Marcxniv" />
      <Button fx:id="rightBtn" layoutX="504.0" layoutY="188.0" mnemonicParsing="false" onAction="#moveRight" text="Marjvniv" />
      <Button fx:id="redBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#makeRed" text="Red button" />
      <Button fx:id="colorCycleBtn" layoutX="19.0" layoutY="350.0" mnemonicParsing="false" onAction="#changeColor" text="Changing colors" />
   </children>
</AnchorPane>
